    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc --noEmit",
    "lint": "yarn eslint && yarn tsc",
    "build-alpha": "git status -s -uall && if ! git diff-index --quiet HEAD --; then echo \"\\033[0;31mPlease commit all changes before the operation! Aborting...\"; exit 1; else cd ios && bundle install && bundle exec fastlane increment_version_build_numbers && bundle exec fastlane update_release_channel && cd ../android && bundle install && bundle exec fastlane increment_version_build_numbers && bundle exec fastlane update_release_channel && cd .. && git add . && git commit -m 'bumped js, ios, android version and build numbers, set the alpha channel' && eas build --platform all --profile alpha --non-interactive; fi",
    "build-prod": "git status -s -uall && if ! git diff-index --quiet HEAD --; then echo \"\\033[0;31mPlease commit all changes before the operation! Aborting...\"; exit 1; else cd ios && bundle install && bundle exec fastlane increment_version_build_numbers && bundle exec fastlane update_release_channel_prod && cd ../android && bundle install && bundle exec fastlane increment_version_build_numbers && bundle exec fastlane update_release_channel_prod && cd .. && git add . && git commit -m 'bumped js, ios, android version and build numbers, set the production channel' && eas build --platform all --profile release --non-interactive; fi",
    "postinstall": "rm -rf node_modules/@draftbit/ui/node_modules/react-native-safe-area-context"
  },
